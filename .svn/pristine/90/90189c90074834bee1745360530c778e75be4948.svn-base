<cordova>
    <!--
    access elements control the Android whitelist.
    Domains are assumed blocked unless set otherwise
     -->

    <access origin="http://127.0.0.1*"/> <!-- allow local pages -->

    <!-- <access origin="https://example.com" /> allow any secure requests to example.com -->
    <!-- <access origin="https://example.com" subdomains="true" /> such as above, but including subdomains, such as www -->
    <!-- <access origin=".*"/> Allow all domains, suggested development use only -->

    <log level="DEBUG"/>
    <preference name="useBrowserHistory" value="false" />
<plugins>
    <plugin name="App" value="org.apache.cordova.App"/>
    <plugin name="Geolocation" value="org.apache.cordova.GeoBroker"/>
    <plugin name="Device" value="org.apache.cordova.Device"/>
    <plugin name="Accelerometer" value="org.apache.cordova.AccelListener"/>
    <plugin name="Compass" value="org.apache.cordova.CompassListener"/>
    <plugin name="Media" value="org.apache.cordova.AudioHandler"/>
    <plugin name="Camera" value="org.apache.cordova.CameraLauncher"/>
    <plugin name="Contacts" value="org.apache.cordova.ContactManager"/>
    <plugin name="File" value="org.apache.cordova.FileUtils"/>
    <plugin name="NetworkStatus" value="org.apache.cordova.NetworkManager"/>
    <plugin name="Notification" value="org.apache.cordova.Notification"/>
    <plugin name="Storage" value="org.apache.cordova.Storage"/>
    <plugin name="Temperature" value="org.apache.cordova.TempListener"/>
    <plugin name="FileTransfer" value="org.apache.cordova.FileTransfer"/>
    <plugin name="Capture" value="org.apache.cordova.Capture"/>
    <plugin name="Battery" value="org.apache.cordova.BatteryListener"/>
    <plugin name="SplashScreen" value="org.apache.cordova.SplashScreen"/>
     <plugin name="HttpRequestPlugin" value="com.ttxc.platform.newPlugin.HttpRequestPlugin"/>
     <plugin name="GetIpAndPortPlugin" value="com.ttxc.platform.newPlugin.GetIpAndPortPlugin"/>
     <plugin name="GobackHome" value="com.ttxc.platform.newPlugin.GobackHomePlugin"/>
     <plugin name="WebviewGobackPlugin" value="com.ttxc.platform.newPlugin.WebviewGobackPlugin"/>
     <plugin name="ShowNetView" value="com.ttxc.platform.newPlugin.ShowNetView"/>
     <plugin name="GetAllCookiesPlugin" value="com.ttxc.platform.newPlugin.GetAllCookiesPlugin"/>
     <plugin name="ScreenPixelPlugin" value="com.ttxc.platform.newPlugin.ScreenPixelPlugin"/>
     <plugin name="LoadingViewPlugin" value="com.ttxc.platform.newPlugin.LoadingViewPlugin"/>
     <plugin name="AltertViewPlugin" value="com.ttxc.platform.newPlugin.AltertViewPlugin"/>
     <plugin name="ErrorViewPlugin" value="com.ttxc.platform.newPlugin.ErrorViewPlugin"/>
     <plugin name="GoToContactPlugin" value="com.ttxc.platform.newPlugin.GoToContactPlugin"/>
     <plugin name="GoToEmailPlugin" value="com.ttxc.platform.newPlugin.GoToEmailPlugin"/>
     <plugin name="LoginWithCertificaiton" value="com.ttxc.platform.newPlugin.LoginWithCer"/>
     <plugin name="GetUsernamePlugin" value="com.ttxc.platform.newPlugin.GetUsernamePlugin"/>
     <plugin name="GetAndroidId" value="com.ttxc.platform.newPlugin.GetAndroidId"/>
     <plugin name="GenChainTicketPlugin" value="com.ttxc.platform.newPlugin.GenChainTicketPlugin"/>
     <plugin name="ExitPlugin" value="com.ttxc.platform.newPlugin.ExitPlugin"/>
     <plugin name="SetIconPlugin" value="com.ttxc.platform.newPlugin.SetIconPlugin"/>
     <plugin name="GetImageIpAndPortPlugin" value="com.ttxc.platform.newPlugin.GetImageIpAndPortPlugin"/>
     <plugin name="GetConnectType" value="com.ttxc.platform.newPlugin.GetConnectType"/>
     <plugin name="GetLoginType" value="com.ttxc.platform.newPlugin.GetLoginType"/>
     <plugin name="LoginWithUsername" value="com.ttxc.platform.newPlugin.LoginWithUsername"/>
    <plugin name="GoToVirturl" value="com.ttxc.platform.newPlugin.GoToVirturlPlugin"/>
    <plugin name="GetLogInfo" value="com.ttxc.platform.newPlugin.GetLogInfoPlugin"/>
    <plugin name="GetShowVirtualPolicy" value="com.ttxc.platform.newPlugin.GetShowVirtualPolicy"/>
